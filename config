ngx_addon_name=ngx_http_uaparser_module

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=ngx_http_uaparser_module
    ngx_module_libs="-lm -luaparser_cpp -lstdc++"
    ngx_module_srcs="$ngx_addon_dir/ngx_http_uaparser_module.cpp"

    . auto/module
else
    CORE_LIBS="$CORE_LIBS -lm -luaparser_cpp -lstdc++"
    HTTP_MODULES="$HTTP_MODULES ngx_http_uaparser_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_uaparser_module.cpp"
fi
